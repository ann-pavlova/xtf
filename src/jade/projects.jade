    body
        .l-wrap
            .b-header.b-header_type_desk
                .container
                    .b-header__inner
                        a(href=url + "index.html").b-header__logo
                            +svg('logo')
                        .b-header__menu
                            ul.b-header__menu-list
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "about.html") about
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "services.html") SERVICES
                                li.b-header__menu-item
                                    a.b-header__menu-link.is-active(href=url + "projects.html") Projects
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "careers.html") Careers
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "contacts.html") Contacts
                        .b-header__phone
                            a(href="tel:+61414084941") +6 141 408 49 41
            .b-header.b-header_type_mob
                .b-header__inner
                    a(href=url + "index.html").b-header__logo
                        +svg('logo')
                    .b-header__mobile-menu-wrap
                        .b-header__mobile-menu
                            ul.b-header__menu-list
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "about.html") about
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "services.html") SERVICES
                                li.b-header__menu-item
                                    a.b-header__menu-link.is-active(href=url + "projects.html") Projects
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "careers.html") Careers
                                li.b-header__menu-item
                                    a.b-header__menu-link(href=url + "contacts.html") Contacts
                        .b-header__phone
                            a(href="tel:+61414084941") +6 141 408 49 41
                    .b-header__hamburger.j-burger
                        span.b-header__hamburger-line
                        span.b-header__hamburger-line
                        span.b-header__hamburger-line
                        span.b-header__hamburger-line
            .container
                .b-page
                    .b-page__inner.b-page__ttl-block.b-projects__ttl-block
                        h1 Projects
                    .b-tabs__list.b-projects__tabs
                        button.j-tabs-link.b-tabs__link.is-active(data-link="residential" data-json=url + "www/json/residential.json") residential
                        button.j-tabs-link.b-tabs__link(data-link="commercial" data-json=url + "www/json/commercial.json") commercial
                        button.j-tabs-link.b-tabs__link(data-link="industrial" data-json=url + "www/json/industrial.json") industrial
                        button.j-tabs-link.b-tabs__link(data-link="privat" data-json=url + "www/json/privat.json") privat
                        button.j-tabs-link.b-tabs__link(data-link="civil" data-json=url + "www/json/civil.json") civil
                    .b-tabs__content
            .b-popup.mfp-hide(id="popup")

            include ../../www/img/sprite/sprite.svg
            script(src="www/scripts/jquery-3.2.1.min.js")
            script(src="www/scripts/app.js")
            script(src="www/scripts/lib/handlebars/handlebars.js")
